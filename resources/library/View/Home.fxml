<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="CENTER" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.controller.graphic.HomeGraphicController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="216.0" minWidth="150.0" prefWidth="216.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="434.0" minWidth="301.0" prefWidth="301.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="208.0" minWidth="148.0" prefWidth="208.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="127.0" minHeight="10.0" prefHeight="124.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="138.0" minHeight="10.0" prefHeight="123.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="105.0" minHeight="10.0" prefHeight="105.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="105.0" minHeight="10.0" prefHeight="105.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <styleClass>
        <String fx:value="grid" />
        <String fx:value="image" />
    </styleClass>
    <stylesheets>
        <URL value="@../Style/Button.css" />
        <URL value="@../Style/HomeGUI.css" />
    </stylesheets>
    <ImageView GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="BASELINE">
        <Image url="@../Images/Library icon.png" />
    </ImageView>
    <HBox alignment="CENTER" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">
        <Button fx:id="signOut" ellipsisString="" mnemonicParsing="false" onAction="#signOutAction" prefHeight="74.0" prefWidth="141.0">
            <graphic>
                <ImageView fitHeight="20.0" fitWidth="23.0">
                    <Image url="@../Images/iconmonstr-log-out-18-240.png" />
                </ImageView>
            </graphic>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
            <font>
                <Font name="Comic Sans MS" size="22.0" />
            </font>
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin>
        </Button>
        <Button fx:id="book" ellipsisString="" mnemonicParsing="false" onAction="#bookAction" prefHeight="74.0" prefWidth="141.0">
            <graphic>
                <ImageView fitHeight="20.0" fitWidth="23.0">
                    <Image url="@../Images/iconmonstr-book-27-240.png" />
                </ImageView>
            </graphic>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
            <font>
                <Font name="Comic Sans MS" size="22.0" />
            </font>
        </Button>
    </HBox>
    <HBox alignment="CENTER" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="CENTER">
        <Button fx:id="report" ellipsisString="" mnemonicParsing="false" onAction="#reportAction" prefHeight="74.0" prefWidth="141.0">
            <graphic>
                <ImageView fitHeight="20.0" fitWidth="23.0">
                    <Image url="@../Images/iconmonstr-book-24-240.png" />
                </ImageView>
            </graphic>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
            <font>
                <Font name="Comic Sans MS" size="22.0" />
            </font>
            <HBox.margin>
                <Insets right="10.0" />
            </HBox.margin>
        </Button>
        <Button fx:id="register" ellipsisString="" mnemonicParsing="false" onAction="#registerAction" prefHeight="74.0" prefWidth="141.0">
            <graphic>
                <ImageView fitHeight="20.0" fitWidth="23.0">
                    <Image url="@../Images/iconmonstr-pen-7-240.png" />
                </ImageView>
            </graphic>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
            <font>
                <Font name="Comic Sans MS" size="22.0" />
            </font>
        </Button>
    </HBox>
</GridPane>
